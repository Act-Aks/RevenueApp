{
  "name": "RevenueApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\"",
    "format": "prettier --config .prettierrc.js --ignore-pattern .prettierignore \"**/*.{cjs,html,js,jsx,ts,tsx,json,md,yml}\"",
    "format:fix": "prettier --config .prettierrc.js --ignore-pattern .prettierignore \"**/*.{cjs,html,js,jsx,ts,tsx,json,md,yml}\" --write",
    "ios": "react-native run-ios",
    "lint": "eslint src \"**/*.{ts,tsx}\"",
    "lint:fix": "eslint src \"**/*.{ts,tsx}\" --fix",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "firebase": "^9.19.0",
    "react": "18.2.0",
    "react-native": "0.71.5",
    "react-native-gesture-handler": "^2.11.0",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-splash-screen": "^3.3.0",
    "styled-components": "^5.3.9"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "^5.1.26",
    "@types/styled-components-react-native": "^5.2.1",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "lint-staged": "^13.2.2",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.4.1",
    "react-native-safe-area-context": "^4.5.3",
    "react-native-screens": "^3.20.0",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,json}": [
      "yarn lint:fix",
      "yarn format:fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
